<div class="preloader" id="loaderD">

    <div class="cube-wrapper">
        <img src="https://aktechindia.com/assets/img/ajax-loader.gif" >
    </div>
</div>
<style>
    #loaderD{
        display: none;
        /*        position: fixed;
                z-index: 999999;
                top: 50%;
                left: 50%;*/
    }
</style>
<script>
    var designer_reload = true;
    $(document).ready(function () {

        $(document).ajaxStart(function () {
            $("#loaderD").css("display", "block");
        });

        $(document).ajaxComplete(function () {
            $("#loaderD").css("display", "none");
        });

        var data_load = $('form#customized_product_form').serialize();
        //filter(data_load);
        getGSMnGusset(data_load);
        $('.custom_print').hide();
        $('.ptype').change(function () {
            var pval = $(this).val();
            if (pval == 'blank') {
                $('.custom_print').hide();
                $('.blank').addClass('active');
                $('.custom').removeClass('active');
            }
            else if (pval == 'custom') {
                $('.custom_print').show();
                $('.custom').addClass('active');
                $('.blank').removeClass('active');
            }
        });
        $('body').on('change', '.confpro', function () {
            var idss = $(this).attr('id');
            designer_reload = checkReload(idss);
            var data = $('form#customized_product_form').serialize();
            filter(data);
        });
        $('.confpronum').bind('keyup mouseup', function () {
            var idss1 = $(this).attr('id');
            designer_reload = checkReload(idss1);
            var data = $('form#customized_product_form').serialize();
            filter(data);
        });
        $('.confpronum').bind('mousewheel', function (e) {
            if ($('.confpronum').is(':focus'))
                e.preventDefault();
        });

    });

    function checkReload(idss) {

        if (idss == 'bag_type' || idss == 'fabric_type' || idss == 'size' || idss == 'handle' || idss == 'bag_colour' || idss == 'stitch_type'
                || idss == 'custom_printed' || idss == 'blank' || idss == 'gusset_colour') {
            return true;
        } else {
            return false;
        }
    }
    function filter(data) {
        var ptype = $('input[name="ptype"]:checked').val();
        $.ajax({
            url: base_url + "shopping/customized-product/filter",
            type: 'POST',
            datatype: 'JSON',
            data: data,
            success: function (data) {
                var show = JSON.parse(data);
                // console.log(show.product);
                $('table#custom_product_table tbody').html(show.html);
                $('#gusset_width').val('');
                if (show.product) {
                    $('#gusset_width').val(show.product['gusset_width']);
                }
                var prodimg = $(show.html).find('#productimg').val();
                if (designer_reload) {
                    if (prodimg)
                        getProductDesigner(prodimg, ptype);
                    else
                        getProductDesigner('', ptype);
                }
            }
        });
    }

    $('.gsminputs').change(function () {
        var data = $('form#customized_product_form').serialize();
        getGSMnGusset(data);
    });

    function getGSMnGusset(data) {
        $.ajax({
            url: base_url + "shopping/customized-product/productgsm",
            type: 'POST',
            datatype: 'JSON',
            data: data,
            success: function (data) {
                $('#gusset_width').val('');
                $('#gusset_colour').val('');
                if (data) {
                    var response = JSON.parse(data);
                    $('.gsmcontainer').html(response['gsmcontent']);
                    if (response['isgusset'] != '0') {
                        $('.gsdiv').show();
                        setSelectedIndex(document.getElementById("gusset_colour"), 1);
                    } else {
                        $('.gsdiv').hide();
                    }
                } else {
                    $('.gsdiv').hide();
                }
                var data_load = $('form#customized_product_form').serialize();
                filter(data_load);
            }
        });
        updateBagName();
    }

    function updateBagName() {
        var bagtype = $('#bag_type').find('option:selected').html();
        var colour = $('#bag_colour').find('option:selected').html();
        var size = $('#size').find('option:selected').html();
        $('.bag-title').html(colour.charAt(0).toUpperCase() + colour.slice(1) + ' ' + bagtype);
    }

    function getProductDesigner(img, ptype) {

        $.ajax({
            url: base_url + "shopping/customized-product/productdesigner",
            type: 'POST',
            data: {'image': img, 'ptype': ptype},
            success: function (data) {
                $('#designer-container').html(data);
            }
        });
    }

    function setSelectedIndex(s, i)
    {
        s.options[i - 1].selected = true;
        return;
    }


</script>

<script src="https://aktechindia.com/assets/js/bootstrap.min.js"></script>
